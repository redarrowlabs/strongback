{
  "name": "@redarrowlabs/strongback-react",
  "version": "0.6.2",
  "description": "Strong UI components, for React",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/redarrowlabs/strongback.git"
  },
  "main": "bin/index",
  "typings": "bin/index",
  "scripts": {
    "build": "npm run build:ts",
    "build:css": "postcss src/third-party.css --use postcss-import autoprefixer --dir ./bin",
    "watch:css": "postcss src/third-party.css --use postcss-import autoprefixer --dir ./bin --watch",
    "build:ts": "tsc",
    "storybook": "start-storybook -p 9001",
    "todo": "leasot --reporter markdown --tags hack --associate-parser .tsx,defaultParser src/**/*.tsx > TODO.md || exit 0",
    "lint:fix": "tslint --fix --config tslint.json src",
    "check": "npm prune && npm install && npm run lint:fix && npm run todo && npm run build",
    "update": "npm-check -u"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "js-joda": "^1.5.1",
    "lodash": "^4.17.4",
    "react-modal": "^1.7.3",
    "react-redux": "^5.0.3",
    "react-s-alert": "^1.2.2",
    "react-select": "1.0.0-rc.2",
    "react-syntax-highlighter": "^5.7.0",
    "react-widgets": "^4.0.0-rc.16",
    "redux-form": "^6.6.1"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-knobs": "^3.2.6",
    "@storybook/addons": "^3.2.6",
    "@storybook/react": "^3.2.6",
    "@types/classnames": "0.0.32",
    "@types/lodash": "^4.14.59",
    "@types/node": "^7.0.12",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.3",
    "@types/react-redux": "^4.4.38",
    "@types/react-select": "^1.0.51",
    "@types/react-widgets": "^3.2.31",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.0",
    "@types/storybook__react": "^3.0.5",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.0",
    "leasot": "^4.3.1",
    "postcss-cli": "^4.1.0",
    "postcss-import": "^10.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "style-loader": "^0.16.1",
    "tachyons": "^4.8.1",
    "ts-loader": "^2.0.3",
    "tslint": "^5.0.0",
    "typescript": "2.4.2"
  }
}
